{
  "name": "@elastosfoundation/did-js-sdk-samples",
  "version": "0.0.0",
  "description": "Elastos DID Typescript SDK Samples",
  "scripts": {
    "build": "npm run clean && npm run types && rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist/ typings/ generated/",
    "types": "npm run clean && tsc --emitDeclarationOnly --outDir typings/ --project tsconfig.types.json",
    "tsc:build": "rollup -c",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "assert": "2.0.0",
    "cd": "0.3.3",
    "url": "0.11.0",
    "web3": "1.7.0",
    "yargs": "17.3.1",
    "@elastosfoundation/did-js-sdk": "^2.2.4"
  },
  "devDependencies": {
    "typescript": "4.5.5",
    "tslib": "2.3.1",

    "@types/node": "14.17.34",
    "@types/yargs": "17.0.8",

    "ts-loader": "9.2.6",
    "ts-node": "9.1.1",

    "rollup": "2.67.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-commonjs": "19.0.2",
    "@rollup/plugin-inject": "4.0.3",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "2.4.2",
    "@rollup/plugin-typescript": "8.3.0",   
    "rollup-plugin-multi-input": "1.3.1"
  },
  "browserslist": "> 0.25%, not dead",
