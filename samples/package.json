{
  "name": "@elastosfoundation/did-js-sdk-samples",
  "version": "0.0.0",
  "description": "Elastos DID Typescript SDK Samples",
  "scripts": {
    "build": "npm run clean && npm run types && rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist/ typings/ generated/",
    "types": "npm run clean && tsc --emitDeclarationOnly --outDir typings/ --project tsconfig.types.json",
    "tsc:build": "rollup -c",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "cd": "^0.3.3",
    "url": "^0.11.0",
    "web3": "^1.3.6",
    "yargs": "^17.0.0",
    "@elastosfoundation/did-js-sdk": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.44",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.1.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.2.4",
    "rollup": "^2.47.0",
    "rollup-plugin-multi-input": "^1.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1"
  },
  "browserslist": "> 0.25%, not dead",
