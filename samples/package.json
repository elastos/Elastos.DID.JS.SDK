{
  "name": "@elastosfoundation/did-js-sdk-samples",
  "version": "0.0.0",
  "description": "Elastos DID Typescript SDK Samples",
  "scripts": {
    "types": "npm run clean && tsc --emitDeclarationOnly --outDir typings/ --project tsconfig.types.json",
    "build": "npm run clean && npm run types",
    "clean": "rm -rf generated/",
    "tsc:build": "rollup -c",
    "simchain": "java -jar tools/didutil.jar simchain --loglevel=trace",
    "updatedidjar": "wget -O tools/didutil.jar https://github.com/elastos/Elastos.DID.Java.SDK/releases/download/release-v2.1.3/didutils.jar"
  },
  "exports": {
    "node" : "./dist/did.js",
    "browser": {
      "import": "./dist/es/did.browser.js"
    }
  },
  "main": "./dist/did.js",
  "module": "./dist/es/did.js",
  "browser": {
    "./dist/did.js": "./dist/did.browser.js",
    "./dist/es/did.js": "./dist/es/did.browser.js"
  },
  "dependencies": {
    "@elastosfoundation/did-js-sdk": "^2.0.0",
    "cd": "^0.3.3",
    "url": "^0.11.0",
    "web3": "^1.3.6",
    "express": "^4.17.1",
    "webpack": "^5.64.2",
    "webpack-dev-middleware": "^5.2.2",
    "webpack-hot-middleware": "^2.25.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^14.14.44",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "rollup": "^2.47.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-multi-input": "^1.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.6.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "tslint-loader": "^3.5.4",
    "ts-loader": "^9.2.6"
  },
  "browserslist": "> 0.25%, not dead",
  "author": "Elastos foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elastos/Elastos.DID.JS.SDK/issues"
  },
  "homepage": "https://github.com/elastos/Elastos.DID.JS.SDK#readme"
}
